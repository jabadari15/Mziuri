    public static int rotateMatrix (int matrix) {
        int n = matrix.length;
        int rotated = new int [n][n];
        for (int i = 0;i<n; i++){
            for ( int j = 0; j<n; j++) {

            }
        }
        return rotated
    }
    public static void printMatrix( int matrix){
        for (int row : matrix){
            for ( int val: row ){
                System.out.print (val);
            }
            System.out.println();
        }
    }
   }
