16. 
 public class BookPriceFinder {
    public static void main (string args) {
        int prices = {1,5,9,2,10,8} 
        int minIndex= 0, maxindex= 0;
        for(int i = 1; < prices.length; i++) {
            if (prices [i]< prices [minIndex]) minIndex=i
            if (prices [i]< prices [maxindex]) maxindex=i

        }
System.out.println (minIndex+ " magaziis yvelaze iafi da jami"+ prices [minIndex]+ "GEL")
System.out.println (maxIndex+ " magaziis yvelaze dzviri da jami"+ prices [maxIndex]+ "GEL")


    }
 }
